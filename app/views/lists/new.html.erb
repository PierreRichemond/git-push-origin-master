<div class="container">
  <div class="row justify-content-center">
    <div class="col-6 ">
      <%= simple_form_for(@list) do |f| %>
        <%= f.input :name %>
        <%= f.input :photo, as: :file %>
        <%= f.submit class: 'btn btn-primary mt-3'%>
      <% end %>
    </div>
  </div>
</div>
