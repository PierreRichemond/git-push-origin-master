<div class="container mt-3">
  <h1>Select your movie-style</h1>
  <% @lists.each do|list| %>
    <div class="d-flex justify-content-between">
      <div class="card" style="width: 18rem;">
        <div class="card-body">
          <%= link_to list.name, list_path(list) %>
        </div>
      </div>
    </div>
  <% end %>
</div>
